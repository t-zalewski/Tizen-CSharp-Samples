<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns="http://tizen.org/ns/packages" api-version="4" package="org.tizen.example.AppHistory" version="1.0.0">
  <profile name="wearable" />
  <ui-application appid="org.tizen.example.AppHistory"
					exec="AppHistory.dll"
					type="dotnet"
					multiple="false"
					taskmanage="true"
					nodisplay="false"
					launch_mode="single">
    <label>AppHistory</label>
    <icon>AppHistory.png</icon>
    <metadata key="http://tizen.org/metadata/prefer_dotnet_aot" value="true" />
  </ui-application>
  <privileges>
    <privilege>http://tizen.org/privilege/apphistory.read</privilege>
  </privileges>
  <provides-appdefined-privileges />
  <feature name="http://tizen.org/feature/app_history">true</feature>
</manifest>
